        <executions>
            <execution>
                <goals>
                    <goal>rev-info</goal>
                </goals>
            </execution>
        </executions>
      </phase>
      <phase>
        <id>generate-resources</id>
        <executions>
            <execution>
                <goals>
                    <goal>build-info</goal>
                    <goal>htmlprops</goal>
                    <goal>stubsprops</goal>
                </goals>
            </execution>
        </executions>
      </phase>
      <phase>
        <id>process-resources</id>
        <executions>
            <execution>
                <goals>
                    <goal>css-compress</goal>
                    <goal>spiffy-jslint</goal>
                    <goal>resources</goal>
                </goals>
            </execution>
        </executions>
      </phase>
      <phase>
        <id>compile</id>
        <executions>
            <execution>
                <goals>
                    <goal>gwt-compile</goal>
                    <goal>closure-compile</goal>
                    <goal>gzip</goal>
                </goals>
            </execution>
        </executions>
      </phase>
    </phases>
  </lifecycle>
</lifecycles>
